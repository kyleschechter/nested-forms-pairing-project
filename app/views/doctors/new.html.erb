<div class="w-100 mx-5">
  <div class="py-5">
    <h1>New Doctor</h1>
  </div>

  <%= form_for @doctor do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.fields_for :patients do |ff| %>
      <%= ff.label :name %>
      <%= ff.text_field :name %>

      <%= ff.label :age %>
      <%= ff.number_field :age %>
    <% end %>

    <%= f.submit %>
  <% end %>
</div>